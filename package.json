{
	"name": "self-util",
	"displayName": "self-util",
	"description": "util",
	"version": "0.0.2",
	"publisher": "TankGq",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:txt"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.FormatTxt",
				"title": "Self Tool: Format Txt"
			},
			{
				"command": "extension.UndoFormatTxt",
				"title": "Self Tool: Undo Format Txt"
			},
			{
				"command": "extension.OpenWebView",
				"title": "Self Tool: Open Web View"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "resourceLangId == txt",
					"command": "extension.FormatTxt"
				},
				{
					"when": "resourceLangId == txt",
					"command": "extension.UndoFormatTxt"
				}
			]
		},
		"languages": [
			{
				"id": "txt",
				"aliases": [
					"txt",
					"TXT"
				],
				"extensions": [
					".txt"
				]
			}
		],
		"grammars": [
			{
				"language": "txt",
				"scopeName": "text.txt",
				"path": "./src/syntaxes/txt.tmLanguage.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	},
	"repository": "https://github.com/Tankgq/self-util"
}
